{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'pdf2docx'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=pdf2docx"
                ],
                "filter": {
                    "name": "pdf2docx",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'nipdf-dump'",
            "cargo": {
                "args": [
                    "build",
                    "-p",
                    "nipdf-dump"
                ]
            },
            "args": [
                "page",
                "-f",
                "../nipdf/sample_files/normal/SamplePdf1_12mb_6pages.pdf",
                "1",
                "--png"
            ],
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}/dump"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'dump-pdf'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=dump-pdf",
                    "--package=pdf2docx"
                ],
                "filter": {
                    "name": "dump-pdf",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration test 'parse_objects_test'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--test=parse_objects_test",
                    "--package=pdf2docx"
                ],
                "filter": {
                    "name": "parse_objects_test",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}